package main

import "strconv"
import "fmt"

// this works better than i would expect.
templ Hello(name string) {
	<div>Hello, { name }</div>
	<ul>
		for i := 0 ; i <= 10; i ++ {
			<li>Potato { strconv.Itoa(i) }</li>
		}
	</ul>
	<div>{ fmt.Sprintf("get") }</div>
	<div>{ strconv.Itoa(34) }</div>
	<div>{ fmt.Sprintf("hello") }</div>
}
